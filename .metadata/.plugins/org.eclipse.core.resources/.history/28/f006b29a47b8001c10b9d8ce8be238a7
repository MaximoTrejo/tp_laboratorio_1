#include <stdio.h>
#include <stdlib.h>
#include "biblioteca.h"


int main(void) {
	setbuf (stdout, NULL);

	float tarjetaC;
	float tarjetaD;
	float btc;
	float precioKm;
	float diferencia;
	menuInicio(&tarjetaC,&tarjetaD,&btc,&precioKm,&diferencia);
	mostrar(&tarjetaC,&tarjetaD,&btc,&precioKm,&diferencia);









	return EXIT_SUCCESS;
}
